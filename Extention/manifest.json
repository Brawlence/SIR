{
	"name": "SIR",
	"description": "SIR is an Image Renamer.\nProvides meaningful names when saving from Pixiv, Twitter, Deviantart, Tumblr, Artstation, Hentai-Foundry, Drawfriends.",
	"icons": {
		"128": "SIR_128x128.png",
		"48": "SIR_48x48.png",
		"16": "SIR_16x16.png"
	},
	"page_action": {
		"default_icon": {
			"16": "SIR_16x16.png",
			"48": "SIR_48x48.png",
			"128": "SIR_128x128.png"
		},
		"default_title": "SIR!"
	},
	"version": "0.8.1",
	"manifest_version": 2,
	"permissions": [ "downloads", "storage", "tabs", "contextMenus" ],
	"content_scripts": [ {
			"matches": [ "*://*.deviantart.com/*", "*://*.deviantart.net/*" ],
			"css": [ "DA/tagsStyle.css" ],
			"run_at": "document_end",
			"js": [ "DA/tagsParser_content_script.js" ]
		},
		{
			"matches": [ "*://*.pixiv.net/*", "*://*.pximg.net/*" ],
			"css": [ "PX/tagsStyle.css" ],
			"run_at": "document_end",
			"js": [ "PX/tagsParser_content_script.js" ]
		},
		{
			"matches": [ "*://*.tumblr.com/*" ],
			"css": [ "TU/tagsStyle.css" ],
			"run_at": "document_end",
			"js": [ "TU/tagsParser_content_script.js" ]
		},
		{
			"matches": [ "*://*.twitter.com/*", "*://*.twimg.com/*" ],
			"css": [ "TW/tagsStyle.css" ],
			"run_at": "document_end",
			"js": [ "TW/tagsParser_content_script.js" ]
		},
		{
			"matches": [ "*://*.artstation.com/*" ],
			"css": [ "AS/tagsStyle.css" ],
			"run_at": "document_end",
			"js": [ "AS/tagsParser_content_script.js" ]
		},
		{
			"matches": [ "*://*.hentai-foundry.com/*", "*://*.hentaifoundry.com/*" ],
			"css": [ "HF/tagsStyle.css" ],
			"run_at": "document_end",
			"js": [ "HF/tagsParser_content_script.js" ]
		},
		{
			"matches": [ "*://drawfriends.booru.org/*s=view*" ],
			"css": [ "DF/tagsStyle.css" ],
			"run_at": "document_end",
			"js": [ "DF/tagsParser_content_script.js" ]
		}
	],
	"background": {
		"scripts": [ "Background.js" ]
	},
	"commands": {
		"SIR_it": {
			"suggested_key": {
				"default": "Ctrl+Shift+1",
				"mac": "Command+Shift+1"
			},
			"description": "Shows the tags string from the image page"
		}
	}
}
